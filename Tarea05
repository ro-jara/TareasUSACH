function obtenerSumaNotas(notas) {
  let suma = 0;
  for (let i = 0; i < notas.length; i++) {
    suma += notas[i];
  }
  return suma;
}

function obtenerPromedioNotas(notas) {
  const suma = obtenerSumaNotas(notas);
  const promedio = suma / notas.length;
  return promedio;
}

const notasAlumno = [6, 8, 9, 2, 5, 10];
console.log("Suma de notas:", obtenerSumaNotas(notasAlumno));
console.log("Promedio de notas:", obtenerPromedioNotas(notasAlumno));
